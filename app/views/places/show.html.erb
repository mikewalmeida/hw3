<h1><%= @place["name"] %></h1>

<h3>Posts</h3>

<ul>
  <% for post in @posts %>
    <li><a href="/posts/<%= post["id"] %>"><%= post["title"] %>   -   <%= post["posted_on"] %></a></li>
  <% end %>
</ul>

<p>
  <a href="/posts/new?place_id=<%= @place["id"] %>">
  <button>
  Add a new post
  </button>
  </a>
</p>


<p><a href="/places">Go back to homepage</a></p>