<h1><%= @place["name"] %></h1>
<h2><%= @post["title"] %></h2>
<h4>When: <%= @post["posted_on"] %></h4>
<h3><%= @post["description"] %></h3>


<p><a href="/places/<%= @place["id"] %>">See other posts from <%= @place["name"] %></a></p>
<p><a href="/places">Go back to homepage</a></p>


<p><%= link_to "Delete this post", @post, method: :delete %></p>




